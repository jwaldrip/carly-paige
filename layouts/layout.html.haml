!!!
%html
  %head
    %title Carly Paige
    = stylesheet_tag :main
    = javascript_tag :main
  %body
    #wrapper
      #header.background.background-1
        .container
          %nav.span-18
            %img.logo{ src: asset_path('logo-dark.png') }
            #header-nav
              #nav-links
                %ul
                  %li= link_to "<img src='#{asset_path('fb-dark.png')}'>", '#facebook'
                  %li= link_to "<img src='#{asset_path('twitter-dark.png')}'>", '#twitter'
                  %li= link_to "<img src='#{asset_path('youtube-dark.png')}'>", '#youtube'
                  %li= link_to "<img src='#{asset_path('itunes-dark.png')}'>", '#itunes'
                  %li= link_to "<img src='#{asset_path('gplus-dark.png')}'>", '#gplus'
              #nav-menu
                %ul
                  %li= link_to 'about', '#about'
                  %li= link_to 'events', '#events'
                  %li= link_to 'music', '#music'
                  %li= link_to 'media', '#media'
                  %li= link_to 'store', '#store'
                  %li= link_to 'contact', '#contact'
      #invisible-line
      #page-content
        = yield
    #footer
      .container
        %img.logo{ src: asset_path('logo-light.png') }
