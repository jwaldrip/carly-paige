#banner.background.background-1
.container
  #highlights
    %ul
      %li
        .inner
          highlight 1
      %li
        .inner
          highlight 2
      %li
        .inner
          highlight 3

.container
  #content
    .clearfix
      #left
        %section#feature-video
          %h1
            %span.inner Feature Video
          .video-container
            %iframe{ width: '100%', height: '100%', src: "//www.youtube.com/embed/ZXqzo06DPaI?wmode=transparent", frameborder: "0", allowfullscreen: true, wmode: "Opaque" }
          .pull-right
            = link_to 'view all videos', '//www.youtube.com/user/carlypaigemusic'
        %section#recent-media
          %h1
            %span.inner Recent Media
        %section#facebook-feed
          %h1
            %span.inner Facebook Feed

      #right
        #latest-instagram
          %h1
            %span.inner Latest Instagram
          %img{ src: asset_path('black-px.png'), width: '100%' }
        #upcoming-events
          %h1
            %span.inner Upcoming Events

    #bottom
      %section#instagram-feed
        %h1
          %span.inner Instagram Feed

:coffee
  delayLoop(5000, cycle_image)
